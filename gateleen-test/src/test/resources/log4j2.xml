<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error">
    <Appenders>
        <Console name="consoleAppender">
            <PatternLayout pattern="%d{ISO8601} gateleen %p %c{1} - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="warn">
            <AppenderRef ref="consoleAppender"/>
        </Root>
    </Loggers>
</Configuration>
